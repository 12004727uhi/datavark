[Unit]
Description=Gunicorn daemon for Django projects
Before=nginx.service
After=network.target

[Service]
WorkingDirectory=/home/django/datareaper/DataReaper
ExecStart=/home/django/.local/share/virtualenvs/DataReaper-FrfBYLl8/bin/gunicorn --name=DataReaper --pythonpath=/home/django/datareaper/DataReaper --bind unix:/home/django/gunicorn-datareaper.socket --config /home/django/datareaper/DataReaper/DataReaper/gunicorn.py DataReaper.wsgi:application
Restart=always
SyslogIdentifier=gunicorn
User=django
Group=django


[Install]
WantedBy=multi-user.target
